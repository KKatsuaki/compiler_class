"//".*|"/*"([^*]|\*+[^*/])*\*+"/" {/*comment*/}
"program" return PROGRAM;
"=" return EQ;
";" return SEMI;
"write" return WRITE;
"int" return INT;
"char" return CHAR;
"," return COLON;
"+" return ADD;
"-" return SUB;
"*" return MUL;
"%" return MOD;
"/" return DIV;
"{" return LBRACE;
"}" return RBRACE;
"(" return LP;
")" return RP;
[a-zA-Z][a-zA-Z0-9]* return IDENT;
[0-9]+ {yylval = atoi(yytext); return NUM;}
[\t\s\n]+ {}
. {}


